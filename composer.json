{
    "name": "fm-labs/cakephp-shop",
    "description": "Shop plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4.4",
        "fm-labs/cakephp-settings": "^1.0.0",
        "fm-labs/cakephp-cron": "^2.0.0",
        "fm-labs/cakephp-content": "*",
        "fm-labs/cakephp-cupcake": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "dms/phpunit-arraysubset-asserts": "^0.4.0"
    },
    "suggest": {
        "fm-labs/cakephp-mpay24": "Enables MPay24/Unzer payment provider support"
    },
    "autoload": {
        "psr-4": {
            "Shop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shop\\Test\\": "tests"
        }
    }
}
