{
    "name": "fm-labs/cakephp3-shop",
    "description": "Ecommerce plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Shop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shop\\Test\\": "tests"
        }
    }
}
